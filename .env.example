# Rename this file to .env.local and fill in the values when integrating with 10Web, Stripe and n8n.

# 10Web API key used when creating and managing websites.
TENWEB_API_KEY=

# n8n webhook URL for automations (not yet integrated).
N8N_WEBHOOK_URL=

# Stripe price IDs for your plans (used later when integrating Stripe checkout). Leave blank in mock mode.
STRIPE_PRICE_STRONG_MONTHLY=
STRIPE_PRICE_STRONG_YEARLY=
STRIPE_PRICE_EXTREME_MONTHLY=
STRIPE_PRICE_EXTREME_YEARLY=